{
  "name": "Document Static renderer",
  "image": "node:lts-alpine",
  "runArgs": [
    "-p=192.168.1.11:4000:4000"
  ],
  "forwardPorts": [4000],
  "postCreateCommand": "npm install",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-vscode-remote.remote-containers",
        "streetsidesoftware.code-spell-checker"
      ]
    }
  }
}